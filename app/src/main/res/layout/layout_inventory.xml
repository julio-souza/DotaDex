<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView_itemMatch_itemSlot1"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_itemMatch_itemSlot2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toEndOf="@id/imageView_itemMatch_itemSlot1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_itemMatch_itemSlot3"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toEndOf="@id/imageView_itemMatch_itemSlot2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_itemMatch_itemSlot4"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toStartOf="@id/imageView_itemMatch_itemSlot1"
        app:layout_constraintTop_toBottomOf="@id/imageView_itemMatch_itemSlot1" />

    <ImageView
        android:id="@+id/imageView_itemMatch_itemSlot5"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toStartOf="@id/imageView_itemMatch_itemSlot2"
        app:layout_constraintTop_toBottomOf="@id/imageView_itemMatch_itemSlot2" />

    <ImageView
        android:id="@+id/imageView_itemMatch_itemSlot6"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toStartOf="@id/imageView_itemMatch_itemSlot3"
        app:layout_constraintTop_toBottomOf="@id/imageView_itemMatch_itemSlot3" />


    <ImageView
        android:id="@+id/imageView_itemMatch_backPack1"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toEndOf="@id/imageView_itemMatch_itemSlot3"
        app:layout_constraintTop_toTopOf="@id/imageView_itemMatch_itemSlot3" />

    <ImageView
        android:id="@+id/imageView_itemMatch_backPack2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toEndOf="@id/imageView_itemMatch_backPack1"
        app:layout_constraintTop_toTopOf="@id/imageView_itemMatch_backPack1" />


    <ImageView
        android:id="@+id/imageView_itemMatch_backPack3"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toStartOf="@id/imageView_itemMatch_backPack1"
        app:layout_constraintTop_toBottomOf="@id/imageView_itemMatch_backPack1" />

    <ImageView
        android:id="@+id/imageView_itemMatch_backPack4"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:contentDescription="@null"
        android:src="@drawable/ic_ancient_five"
        app:layout_constraintStart_toEndOf="@id/imageView_itemMatch_backPack3"
        app:layout_constraintTop_toTopOf="@id/imageView_itemMatch_backPack3" />

</androidx.constraintlayout.widget.ConstraintLayout>
